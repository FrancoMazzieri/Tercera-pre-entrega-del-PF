<div>
    <div>

        <h1>Bienvenido </h1>
        <h2>Datos del perfil:</h2>
        <div>Nombre: {{user.firstName}}</div>
        <div>Apellido: {{user.lastName}}</div>
        <div>Edad: {{user.age}}</div>
        <div>Email: {{user.email}}</div>
    </div>

    <form action="../auth/logout" method="post">
        <button id="logout" class="btnInverted">Cerrar sesion</button>
    </form>
</div>

<div>
    <h2>Todos los Productos</h2>
    {{#each productos}}
    <div>
        <h3>Titulo: {{this.title}}</h3>
        <p>Descripcion: {{this.description}}</p>
        <p>Precio: {{this.price}}</p>
        <p>Cantidad: {{this.stock}}</p>
        <p>Id: {{this.id}}</p>
    </div>
    {{/each}}
</div>